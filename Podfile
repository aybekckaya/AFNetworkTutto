# platform :ios, '9.0'

use_frameworks!

def default_pods
  pod 'AFNetworking', '~> 3.0'
end

target 'AFNetworkTutto' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for AFNetworkTutto
default_pods

  target 'AFNetworkTuttoTests' do
    inherit! :search_paths
    # Pods for testing
default_pods
  end

  target 'AFNetworkTuttoUITests' do
    inherit! :search_paths
    # Pods for testing
default_pods
  end

end
